# Environment variables for x402-express-mantle example server
# Copy this file to .env and fill in your values

# Your EVM address to receive payments on Mantle testnet
EVM_ADDRESS=0x...

# URL of your x402 facilitator (deploy to Vercel first)
FACILITATOR_URL=https://your-facilitator.vercel.app

# Optional: Port to run the server on (default: 3000)
PORT=3000
